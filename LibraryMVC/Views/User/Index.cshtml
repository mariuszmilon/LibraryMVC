@model IEnumerable<LibraryMVC.Models.BorrowedBookDto>

<h1>Your books</h1>
<h4>See the books you borrowed below</h4>
<partial name="_TempDataView" />

<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                Author
            </th>
            <th>
                Date of borrowing
            </th>
            <th>
                Date of returning
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Author)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Start)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Stop)
            </td>
        </tr>
        }
    </tbody>
</table>